{
    "manifest_version": 2,
    "name": "Productify",
    "version": "1.0",
  
    "description": "Keeps track of productivity",
  
    "icons": {
      "48": "icons/clock.png"
    },
    
    "permissions": ["activeTab"],

    "browser_action": {
        "default_icon": "icons/clock.png",
        "default_title": "Productify"
      },

    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["productify.js"]
      }
    ]
  }
  